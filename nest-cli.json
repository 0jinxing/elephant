{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/elephant",
  "monorepo": true,
  "root": "apps/elephant",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/elephant/tsconfig.app.json"
  },
  "projects": {
    "elephant": {
      "type": "application",
      "root": "apps/elephant",
      "entryFile": "main",
      "sourceRoot": "apps/elephant",
      "compilerOptions": {
        "tsConfigPath": "apps/elephant/tsconfig.app.json"
      }
    },
    "rss-feed": {
      "type": "application",
      "root": "apps/rss-feed",
      "entryFile": "main",
      "sourceRoot": "apps/rss-feed",
      "compilerOptions": {
        "tsConfigPath": "apps/rss-feed/tsconfig.app.json"
      }
    },
    "rss-rule": {
      "type": "application",
      "root": "apps/rss-rule",
      "entryFile": "main",
      "sourceRoot": "apps/rss-rule",
      "compilerOptions": {
        "tsConfigPath": "apps/rss-rule/tsconfig.app.json"
      }
    }
  }
}